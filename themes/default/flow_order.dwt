function submitcheck(frm)
{
  var msg = new Array();
  var err = false;

  if (frm.elements['dh_name'] && frm.elements['dh_name'].value == '')
  {
    msg.push("{$lang.plase_inputsubscriber_name}");
    err = true;
  }
  if(frm.elements['hk_fuyanlan'] && frm.elements['hk_fuyanlan'].value.length > 150) 
  {
	msg.push("贺卡内容不能超过150个字符");
    err = true;
  }
  if (frm.elements['dhr_nomarch'] && frm.elements['dhr_nomarch'].value == ''&&frm.elements['dhr_area'] && frm.elements['dhr_area'].value == '')
  {
    msg.push("请选择订货人省市");
    err = true;
  }
  if((frm.elements['rd_self'].item(0).checked)==false){
  if(frm.elements['ps_address'].value==0 || frm.elements['ps_address'].value==''){
	  if (frm.elements['shr_nomarch'] && frm.elements['shr_nomarch'].value == ''&&frm.elements['shr_area'] && frm.elements['shr_area'].value == '')
	  {
		msg.push("请选择收货人省市");
		err = true;
	  }
  }
  }
  if (frm.elements['dxxaddress'] && frm.elements['dxxaddress'].value == '')
  {
    err = true;
    msg.push("{$lang.plase_inputsubscriber_address}");
  }else if(frm.elements['dxxaddress'] && frm.elements['dxxaddress'].value.length<6)
  {
  	err = true;
    msg.push("{$lang.subscriber_address_no}");
  }

  if (frm.elements['dmobile'] && frm.elements['dmobile'].value =='')
  {
      err = true;
      msg.push("{$lang.plase_inputsubscriber_phone}");
  }else
  {
  	if(!checkMobile(frm.elements['dmobile'].value))
	{
		err = true;
      	msg.push("{$lang.subscriber_mobilephone_no}");
	}
  }
  
  if(frm.elements['rd_self'] && frm.elements['rd_self'].length>1)
  {
  	if(frm.elements['rd_self'].item(0).checked && frm.elements['rd_self'].item(0).value=='1')
	{
		if(frm.elements['self_dizhi'] && frm.elements['self_dizhi'].length>1)
		{
			var ress=false;
			for(var j=0;j<frm.elements['self_dizhi'].length;j++)
			{
				if(!frm.elements['self_dizhi'].item(j).checked)
				{
					ress=true;
				}else
				{
					ress=false;
					break;
				}
			}
			if(ress)
			{
				err=true;
				msg.push("{$lang.plase_chosemyselfaddress}");
			}
		}
	}else if(frm.elements['rd_self'].item(1).checked && frm.elements['rd_self'].item(1).value=='0')
	{
		if(frm.elements['ps_address'] && frm.elements['ps_address'].length==undefined)
		{
			if(frm.elements['ps_address'].checked && frm.elements['ps_address'].value=='0')
			{
				if(frm.elements['sh_name'] && frm.elements['sh_name'].value=='')
						{
							err=true;
							msg.push("{$lang.plase_inputccinfo_name}");
						}
						
						if(frm.elements['sxxaddress'] && frm.elements['sxxaddress'].value=='')
						{
							err=true;
							msg.push("{$lang.plase_inputccinfo_address}");
						}else if(frm.elements['sxxaddress'].value.length<6)
						{
							err=true;
							msg.push("{$lang.ccinfo_address_length_no}");
						}
						
						if(frm.elements['smobile'] &&　frm.elements['smobile'].value=='')
						{
							err=true;
							msg.push("{$lang.plase_inputccinfo_phone}");
						}else if(!checkMobile(frm.elements['smobile'].value))
						{
							err=true;
							msg.push("{$lang.ccinfo_phone_no}");
						}
			}else
			{
				err=true;
				msg.push("{$lang.plase_chose_other_address}");
			}

		}else if(frm.elements['ps_address'] && frm.elements['ps_address'].length>1)
		{
			var res=false;
			for(var i=0;i<frm.elements['ps_address'].length;i++)
			{
				if(frm.elements['ps_address'].item(i).checked)
				{
					res=true;
					if(frm.elements['ps_address'].item(i).value=='0')
					{
						if(frm.elements['sh_name'] && frm.elements['sh_name'].value=='')
						{
							err=true;
							msg.push("{$lang.plase_inputccinfo_name}");
						}
						
						if(frm.elements['sxxaddress'] && frm.elements['sxxaddress'].value=='')
						{
							err=true;
							msg.push("{$lang.plase_inputccinfo_address}");
						}else if(frm.elements['sxxaddress'].value.length<6)
						{
							err=true;
							msg.push("{$lang.ccinfo_address_length_no}");
						}
						
						if(frm.elements['smobile'] &&　frm.elements['smobile'].value=='')
						{
							err=true;
							msg.push("{$lang.plase_inputccinfo_phone}");
						}else if(!checkMobile(frm.elements['smobile'].value))
						{
							err=true;
							msg.push("{$lang.ccinfo_phone_no}");
						}
					}
					break;
				}else
				{
					res=false;
				}
			}
			if(!res)
			{
				err=true;
				msg.push("{$lang.plase_chose_default_or_other_address}");
			}
		}
	}else
	{
		err=true;
		msg.push("{$lang.plase_chose_peisong_type}");
	}
  }
    
  if(frm.elements['needfapiao'] && frm.elements['needfapiao'].checked)
  {
  	if(frm.elements['gs_name'] && frm.elements['gs_name'].value=='')
	{
		err=true;
		msg.push("{$lang.plase_inputcompany_name}");
	}
	if(frm.elements['fp_name'].item(0).checked==false && frm.elements['fp_name'].item(1).checked==false && frm.elements['fp_name'].item(2).checked==false && frm.elements['fp_name'].item(3).checked==false)
	{
		err=true;
		msg.push("{$lang.plase_chose_fapiao_name}");
	}
  }
  
  if(frm.elements['sh_time'] && frm.elements['sh_time'].value=='')
  {
		err=true;
		msg.push("{$lang.plase_input_songhuo_tiem}");
  }
  if (err)
  {
    message = msg.join("\n");
    alert(message);
  }
  return ! err;

}
