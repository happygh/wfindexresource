			map.centerAndZoom(point, 9);                 // 初始化地图，设置中心点坐标和地图级别
		}
		
		// 创建地址解析器实例  
		var myGeo = new BMap.Geocoder();  
		// 将地址解析结果显示在地图上，并调整地图视野  
		myGeo.getPoint(myaddress, function(point){
			get_polygon(point,myaddress);
			map.addOverlay(new BMap.Marker(point));  
		}, cityName);
	}
	//根据城市名称获得城市免费区域块
	function get_polygon(p,maddr)
	{
		var point = p;
		var aaa = '';
		
		if(is_defalut_add==1)
		{
			city = (maddr.substr(0,3)=='北京市'?'北京':'上海');
		}
		else
		{
			city = (document.getElementById("shr_nomarch").value==1?'北京':'上海');
		}
		//alert(city+"==="+maddr);
		$.ajax({
			type: "POST",
			url: "flow_order.php",
			dataType: 'json',
			data: "step=get_freePlygon&city="+city,
			success: function(msg){
				map.clearOverlays();
				var plygonStr = '';
				var resTr = '';
				for(var fr=0; fr<msg['message'].length;fr++)
				{
					plygonStr = plygonStr + "polygon_"+msg['message'][fr]['quyu_id']+" = new BMap.Polygon(["
					for(var fp=0; fp<msg['message'][fr]['points'].length;fp++)
					{	
						plygonStr=plygonStr+"new BMap.Point("+(msg['message'])[fr]['points'][fp]+"),"
					}
					plygonStr=plygonStr+"new BMap.Point("+(msg['message'])[fr]['points'][0]+")"
					plygonStr = plygonStr + "], {strokeColor:'blue', strokeWeight:6, strokeOpacity:0.5});map.addOverlay(polygon_"+msg['message'][fr]['quyu_id']+");";
					resTr = resTr + "BMapLib.GeoUtils.isPointInPolygon(p,polygon_"+msg['message'][fr]['quyu_id']+")||";
				}
				resTr = "("+resTr+"false)";
				eval(plygonStr);
				if(eval(resTr))
				{
					document.getElementById("psqy_bz").style.display='none';
					$("#wuhuan_bz").val(0);
					$("#zongyunfeiwf").html("0元");
					$("#Deliver_Fee").val(0)
				}
				else
				{
					$("#wuhuan_bz").val(1);
					document.getElementById("peisongtishi").style.display="block";
					document.getElementById("peisongshtishi").style.display="none";
					document.getElementById("psqy_bz").style.display='block';
					$("#zongyunfeiwf").html("15元");
					$("#Deliver_Fee").val(15);
				}
			}
		});
	}
	
